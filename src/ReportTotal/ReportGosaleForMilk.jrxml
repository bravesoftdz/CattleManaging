	<queryString><![CDATA[select 
     cp.number_pelastic as numberPelastic ,
     (select (select clb.location_name from c_location_base clb where clb.c_location_base_id = clc.c_location_cow_id) from c_location_cow clc where clc.c_property_id = cp.c_property_id) as locationName,
     to_nchar(nvl(cp.date_birthday,sysdate),'yyyy/mm/dd','nls_calendar = persian') as birthdate,
     to_nchar(sysdate,'yyyy/mm/dd','nls_calendar = persian') as currentDate,
     round(sysdate - cp.date_birthday ) as roz
  from c_property cp
 where cp.active = 1
   and cp.dore = 1
   and sysdate - cp.date_birthday >= $P{numberDay}]]></queryString>

