	<parameter name="minDay" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="maxDay" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[select round(sysdate - ctl.date_talghih)as numberDay,
       (select cp.number_body from c_property cp where cp.c_property_id = ctl.c_property_id )as cow_id,
       (select (select clb.location_name from c_location_base  clb where clb.c_location_base_id = clc.c_location_base_id) as locationName from c_location_cow clc where clc.c_property_id = ctl.c_property_id) as locationCow,
       (select to_nchar(ctl.date_talghih,'yyyy/mm/dd','nls_calendar=persian') from dual)  as dateTalghih,
       (select to_nchar(sysdate,'yyyy/mm/dd','nls_calendar=persian') from dual)  as currentDate,
       (select to_nchar(ct8.test_date,'yyyy/mm/dd','nls_calendar=persian') from c_test ct8 where ct8.c_talghih_id = ctl.c_talghih_id) as testDate
 from c_talghih  ctl
 where
round(sysdate - ctl.date_talghih)>= $P{minDay}
and 
round(sysdate - ctl.date_talghih) <= $P{maxDay}
and
ctl.date_talghih = (select max(ct9.date_talghih)from c_talghih ct9 where ct9.c_talghih_id = ctl.c_talghih_id )
and
(select count(*) from c_test ct5 where ct5.c_talghih_id = ctl.c_talghih_id) = 1
and

(select ct3.test_result_id from c_test ct3 where ct3.c_talghih_id = ctl.c_talghih_id) = (select tr.test_result_id from test_result  tr where trim(tr.name) = trim('P+'))]]></queryString>

