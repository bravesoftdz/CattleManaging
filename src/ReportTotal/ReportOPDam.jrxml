       (select to_nchar(sysdate,'yyyy/mm/dd','NLS_CALENDAR=PERSIaN')from dual) as currentDate
from c_property clc
where  
trim(clc.cow_sex) = trim('ماده')
and
clc.active = 1
and
(select selectcowforop(clc.number_body) from dual) <> 0]]></queryString>

