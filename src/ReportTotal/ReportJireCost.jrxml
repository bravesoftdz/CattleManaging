	<parameter name="jireName" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select 
    cj.jire_name as jireName,
    (select cfb.food_name from c_food_base cfb where cfb.c_food_base_id = cjd.c_food_base_id )as foodName,
    (cjd.food_weight)as weightFood ,
    to_nchar(sysdate,'yyyy/mm/dd','nls_calendar = persian') as currentDate,
    to_nchar(cj.jire_date,'yyyy/mm/dd','nls_calendar = persian') as jireDate,
    ( select cs.price from c_store cs where cs.c_food_base_id = cjd.c_food_base_id and cs.store_date = (select max(cs1.store_date) from c_store cs1 where cs1.c_food_base_id = cjd.c_food_base_id)) as costFood,
     (( select cs.price from c_store cs where cs.c_food_base_id = cjd.c_food_base_id and cs.store_date = (select max(cs1.store_date) from c_store cs1 where cs1.c_food_base_id = cjd.c_food_base_id)) * (cjd.food_weight) )as costFoodJire,
    jireCost(c_jire_id) as jirePrice
from c_jire cj inner join c_jire_daescription cjd using (c_jire_id)
 where trim(cj.jire_name) = trim($P{jireName})
]]></queryString>

