    (select to_nchar(cz.date_khoshk,'yyyy/mm/dd','NLS_CALENDAR=PERSIAN') from dual) as khoshkDate,
    (select to_nchar( $P{endDate},'yyyy/mm/dd','NLS_CALENDAR=PERSIAN') from dual) as dateCurrent,
    (select sc1.name from status_child sc1 where sc1.status_child_id = cz.status_child_id) as statusChild ,
    (select to_nchar($P{startDate},'yyyy/mm/dd','NLS_CALENDAR=PERSIAN') from dual)as startdate,
    (select csb.status_name from c_status_base csb where csb.c_status_base_id = cz.dore) as dore
 from c_zayeman cz
where cz.zayaman_date > $P{startDate}
and cz.zayaman_date < $P{endDate}
order by cz.zayaman_date]]></queryString>

