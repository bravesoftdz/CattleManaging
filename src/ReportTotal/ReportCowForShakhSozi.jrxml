	<queryString><![CDATA[select
 cp.number_body as numberBody,
 (select (select clb.location_name from c_location_base  clb where clb.c_location_base_id = clc.c_location_base_id) as locationName 
              from c_location_cow clc where clc.c_property_id = cp.c_property_id
          ) as locationCow,
  to_nchar(cp.date_birthday,'yyyy/mm/dd','nls_calendar=persian')  as birthday,
  to_nchar(sysdate,'yyyy/mm/dd','nls_calendar=persian')  as currentDate,
 round( sysdate-cp.date_birthday ) as numberDate
from c_property cp 
where 
cp.active = 1 
and 
(cp.flag_shakhsozi = 0 or cp.flag_shakhsozi is null)
order by numberDate]]></queryString>

