	<queryString><![CDATA[select cp.number_body as numberBody,
       (select convertdatemiladitofarsi(max(cz.zayaman_date)) from c_zayeman cz where cz.c_property_id = cp.c_property_id) as dateZayeman,  
       (select round(sysdate-max(cz.zayaman_date)) from c_zayeman cz where cz.c_property_id = cp.c_property_id) as numberDate  ,
       convertdatemiladitofarsi(sysdate) as currentdate,
       (select (select clb.location_name  from c_location_base clb where clb.c_location_base_id = clc.c_location_base_id) from c_location_cow clc where clc.active = 1 and clc.c_property_id = cp.c_property_id) as locationName,
       (select csb.status_name from c_status_base csb where csb.c_status_base_id = cp.dore) as dore
  from c_property cp
 where cp.dore > 5
 and cp.active = 1 
   and TalghihAfterZayemen(cp.c_property_id) = 0
   and trim(cp.cow_sex) = trim('ماده')
   order by datezayeman]]></queryString>

