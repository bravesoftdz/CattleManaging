	<parameter name="endDate" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select 
       (select to_nchar(ct.date_talghih,'yyyy/mm/dd','NLS_CALENDAR=PERSIAN') from dual) as dateTalghih,
       (select to_nchar($P{endDate},'yyyy/mm/dd','NLS_CALENDAR=PERSIAN') from dual) as currentdate, 
       (select cbs.number_sperm from c_base_sperm cbs where cbs.c_base_sperm_id = ct.c_base_sperm_id) as nameSperm,
       (select cp.number_body from c_property cp where cp.c_property_id = ct.c_property_id) as numberBody,
       (select (select  tr.name from test_result tr where tr.test_result_id =ct2.test_result_id) from c_test ct2 where ct2.c_talghih_id = ct.c_talghih_id and ct2.test_date = (select max(ct4.test_date)from c_test ct4 where ct4.c_talghih_id = ct.c_talghih_id)) as testresult,
       (Select cpr.last_name from c_personal cpr where cpr.c_personal_id = ct.c_personal_id) as namePerson,
       (select csb.status_name from c_status_base csb where csb.c_status_base_id = ct.dore)as dore,
       (select to_nchar($P{startDate},'yyyy/mm/dd','NLS_CALENDAR=PERSIAN') from dual)as startDate
from c_talghih ct
where
 ct.date_talghih > $P{startDate}
and
 ct.date_talghih <$P{endDate}
and 
 ct.c_base_sperm_id is not null 
order by ct.date_talghih]]></queryString>

